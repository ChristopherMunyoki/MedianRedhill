mixin navbar-default(obj)
	header.page-header.rd-navbar-default.rd-navbar-transparent(class=obj.navbarClass)
		// RD Navbar
		.rd-navbar-wrap
			nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-md-layout='rd-navbar-fullwidth', data-md-device-layout='rd-navbar-fixed', data-lg-layout='rd-navbar-static', data-lg-device-layout="rd-navbar-static",  data-md-stick-up-offset="80px", data-lg-stick-up-offset="46px" data-stick-up='true' data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true')
				.rd-navbar-collapse-toggle(data-rd-navbar-toggle='.rd-navbar-collapse')
					span
				.rd-navbar-inner
					// RD Navbar Panel
					.rd-navbar-panel
						// RD Navbar Toggle
						button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap')
							span
						// RD Navbar Brand
						.rd-navbar-brand
							a.brand-name(href='./')
								+png(logoDefault)

					.rd-navbar-aside-right
						.rd-navbar-nav-wrap
							include ../_nav

						.rd-navbar-aside-social
							//RD Navbar Search
							.rd-navbar-search
								a.rd-navbar-search-toggle(data-rd-navbar-toggle=".rd-navbar-search" href="#")
									span
								form.rd-search(action="search-results.html" data-search-live="rd-search-results-live" method="GET")
									div.form-wrap
										label.form-label(for="rd-navbar-search-form-input" class="form-label") Search...
										input(class="rd-navbar-search-form-input form-input" id="rd-navbar-search-form-input" type="text" name="s" autocomplete="off")
										#rd-search-results-live.rd-search-results-live
									button.rd-search-form-submit.fl-budicons-launch-search81

							a(href="cart-page.html").basket.mdi-cart-outline
								span 2